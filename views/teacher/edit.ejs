<h4>Editing Form for Student Roll No :<%= student.rollno %>
</h4>
<form method="POST" class="addStu" autocomplete="off" action="/update">
  <input type="hidden" value="<%= student.rollno %>" name="rollno">
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Name : </label>
    <input type="text" name="name" required class="form-control" value="<%= student.name  %> " id="exampleInputEmail1">
  </div>

  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Date of Birth : </label>
    <input type="date" name="dob" value="<%= student.dob%> " required class="form-control" id="exampleInputEmail1">
  </div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Score : </label>
    <input type="number" name="score" value="<%= student.score  %> " required class="form-control"
      id="exampleInputEmail1">
  </div>
  <button type="submit" class="btn btn-success">Update</button>
</form>